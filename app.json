{
  "expo": {
    "name": "Doroosi",
    "slug": "App",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "app",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.salma_sobhi.Doroosi",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
         "backgroundColor": "transparent"
      }
    },
    "web": {
      "output": "single"
    },
    "plugins": [
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "b18a58be-c477-4a2e-b9b2-420fd9a06642"
      }
    },
     "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b18a58be-c477-4a2e-b9b2-420fd9a06642"
    }
  }
}
